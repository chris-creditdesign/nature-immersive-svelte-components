{
  "name": "nature-immersive-svelte-components",
  "version": "0.5.13",
  "description": "Collection of svelte components to build nature immersive articles",
  "svelte": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
